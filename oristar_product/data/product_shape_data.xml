        <!-- Material Category -->
        <record id="product_basic_shape_00" model="product.basic.shape">
            <field name="name">Cuộn</field>
            <field name="has_size1">True</field>
            <field name="has_size2">True</field>
            <field name="has_size3">False</field>
            <field name="has_weight">True</field>
            <field name="is_compact">False</field>
        </record>
        <record id="product_basic_shape_01" model="product.basic.shape">
            <field name="name">Tấm</field>
