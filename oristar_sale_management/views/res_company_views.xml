<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_company_form" model="ir.ui.view">
		<field name="name">view.company.form.inherit.sale.oristar</field>
		<field name="model">res.company</field>
		<field name="priority" eval="55" />
